{
  "name": "repos",
  "description": "Tiny wrapper around github-base for getting publicly available information for a repository, or all of the repositories for one or more users or orgs, from the GitHub API.",
  "version": "2.0.0",
  "homepage": "https://github.com/jonschlinkert/repos",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/repos",
  "bugs": {
    "url": "https://github.com/jonschlinkert/repos/issues"
  },
  "license": "MIT",
  "files": [
    "bin",
    "index.js"
  ],
  "main": "index.js",
  "bin": {
    "repos": "bin/cli.js"
  },
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "github-base": "^1.0.0",
    "minimist": "^1.2.0",
    "orgs": "^1.0.0",
    "write": "^1.0.3"
  },
  "devDependencies": {
    "data-store": "^1.0.0",
    "mocha": "^3.5.3",
    "gulp-format-md": "^1.0.0"
  },
  "keywords": [
    "all",
    "allpublic",
    "api",
    "commits",
    "create",
    "del",
    "delete",
    "destroy",
    "download",
    "edit",
    "fork",
    "forks",
    "get",
    "github",
    "isstarred",
    "list",
    "paginate",
    "paginated",
    "patch",
    "post",
    "put",
    "repos",
    "repositories",
    "repository",
    "request",
    "revision",
    "star",
    "starred",
    "unstar"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "gists",
        "github-base",
        "github-content",
        "github-contributors",
        "topics"
      ]
    },
    "lint": {
      "reflinks": true
    }
  }
}
